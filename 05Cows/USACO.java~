		    throw new IllegalArgumentException("First row not formatted correctly");
		}
		if (firstLine.hasNextInt()){
		    e = firstLine.nextInt();
		}
		else{
		    throw new IllegalArgumentException("First row not formatted correctly");
		}
		if (firstLine.hasNextInt()){
		    n = firstLine.nextInt();
		}
		else{
		    throw new IllegalArgumentException("First row not formatted correctly");
		}
	    }
	    else{
		throw new IllegalArgumentException("File not formatted correctly");
	    }
